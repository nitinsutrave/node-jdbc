{
  "name": "@nitinsutrave/jdbc",
  "version": "0.8.0",
  "description": "use jdbc from node",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "homepage": "https://github.com/nitinsutrave/node-jdbc",
  "bugs": {
    "email": "m.neundorf@live.de",
    "url": "https://github.com/nitinsutrave/node-jdbc/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nitinsutrave/node-jdbc.git"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tslint src",
    "lint:fix": "npm run tslint -- --fix",
    "prepublish": "npm run build",
    "maven": "node-java-maven",
    "pretest": "npm run maven",
    "test": "mocha",
    "test:coverage": "nyc npm test"
  },
  "java": {
    "dependencies": [
      {
        "groupId": "com.h2database",
        "artifactId": "h2",
        "version": "1.4.195"
      }
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/should": "^8.3.0",
    "co-mocha": "^1.2.0",
    "java": "^0.8.0",
    "mocha": "^3.3.0",
    "node-java-maven": "^0.0.12",
    "nyc": "^10.3.2",
    "should": "^11.2.1",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "tslint-config-standard": "^5.0.2",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.3",
    "@types/debug": "0.0.29",
    "@types/java": "^0.7.31",
    "@types/lodash": "^4.14.64",
    "@types/node": "^7.0.29",
    "bluebird": "^3.5.0",
    "deasync": "^0.1.9",
    "debug": "^2.6.8",
    "lodash": "^4.17.4"
  },
  "peerDependencies": {
    "java": "^0.8.0"
  },
  "optionalDependencies": {
    "node-java-maven": "^0.0.12"
  }
}
